<?xml version="1.0" encoding="ISO-8859-1" ?>
<widget xmlns       = "http://www.w3.org/ns/widgets"
    xmlns:gap       = "http://phonegap.com/ns/1.0"
    xmlns:android   = "http://schemas.android.com/apk/res/android"
    id              = "com.beyoubeglobal"
    version         = "1.0.0">

   <name>Be you be global</name>
   <description>Be you be global, online english course.</description>
   <author href="https://www.nettingsolutions.com" email="pgutierrez@nettingsolutions.com">Pedro Gutierrez</author>



   <gap:plugin name="org.apache.cordova.device" />

   <preference name="phonegap-version"           value="3.4.0" />          <!-- all: current version of PhoneGap -->
   <preference name="orientation"                value="default" />        <!-- all: default means both landscape and portrait are enabled -->
   <preference name="target-device"              value="universal" />      <!-- all: possible values handset, tablet, or universal -->
   <preference name="fullscreen"                 value="true" />

   <gap:platform name="android" />

   <icon src="icon.png" />
    <icon src="res/drawable-mdpi/ic_launcher.png"   gap:platform="android"    gap:qualifier="mdpi" />
    <icon src="res/drawable-hdpi/ic_launcher.png"   gap:platform="android"    gap:qualifier="hdpi" />
    <icon src="res/drawable-hxdpi/ic_launcher.png"  gap:platform="android"    gap:qualifier="xhdpi" />

</widget>